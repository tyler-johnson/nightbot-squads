{
  "parser": "babel-eslint",
  "extends": [ "eslint:recommended" ],
  "rules": {
    "semi": "warn",
    "no-console": "off",
    "no-shadow": "warn"
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  }
}
